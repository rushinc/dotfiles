#!usr/bin/bash

if [ "$HOSTNAME" = archPad ]; then
	xinput disable 10 &
fi
if [ "$HOSTNAME" = archStation ]; then
	/home/rushin/.screenlayout/defaults.sh &
fi
numlockx &

# Services
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
compton --respect-prop-shadow &
mopidy &
mpd-notification &
mpdscribble &
pulseeffects --gapplication-service &
nitrogen --restore &

# Tray applets
nm-applet &
xfce4-power-manager &
udiskie -s &
clipit &
